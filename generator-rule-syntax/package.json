{
  "name": "dralee-generator-rule-syntax",
  "displayName": "Dralee Generator Rule Syntax Highlighter",
  "description": "遊龍生成器规则语法高亮",
  "icon": "images/hand.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "generator",
    "config",
    "code",
    "syntax",
    "highlighting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dralee/dralee-vscode-extensions/generator-rule-syntax"
  },
  "license": "Apache-2.0",
  "contributes": {
    "languages": [
      {
        "id": "dralee-generator-rule",
        "aliases": ["Dralee generator rule", "draleeegeneratorrule", "dr"],
        "extensions": [".dr"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dralee-generator-rule",
        "scopeName": "source.dr",
        "path": "./syntaxes/dr.tmLanguage.json"
      }
    ]
  }
}
