{
  "name": "dralee-content-replace-config-syntax",
  "displayName": "Dralee Content Replace Config Syntax Highlighter",
  "description": "自定义内容替换配置语法高亮",
  "icon": "images/code-optimization.png",
  "version": "0.0.1",
  "publisher": "dralee",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "enums",
    "config",
    "code",
    "syntax",
    "highlighting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dralee/dralee-vscode-extensions/replace-config-syntax"
  },
  "license": "Apache-2.0",
  "contributes": {
    "languages": [
      {
        "id": "tr",
        "aliases": ["Dralee Content Replace Config", "draleeecontentreplaceconfig", "tr"],
        "extensions": [".tr"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tr",
        "scopeName": "source.tr",
        "path": "./syntaxes/tr.tmLanguage.json"
      }
    ]
  }
}
